@model IEnumerable<Timetable.Domains.Headteacher.TimetableHead>

@{
    ViewData["Title"] = "Create";
}
@ViewData["className"]
<h1>Выберите день недели</h1>

<table class="table">
    <thead>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a class="btn btn-info text-white" asp-controller="Informer" asp-action="TimetableFinal" asp-route-id="@item.Id">@Html.DisplayFor(modelItem => item.Day)</a>
                </td>
            </tr>
        }
    </tbody>
</table>